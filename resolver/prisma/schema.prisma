// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Resource {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  url      String
  relation String
  titile   String
  language String?
  media    String?
  context  String?
}
